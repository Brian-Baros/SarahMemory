{
  "id": "com.softdev0.printer.generic.tcp",
  "name": "Generic Network Printer Driver",
  "type": "driver",
  "version": "1.0.0",
  "author": "SOFTDEV0 LLC / Brian Lee Baros",
  "description": [
    {
      "id": "connection",
      "title": "Connection",
      "fields": [
        {
          "key": "host",
          "label": "Printer Host/IP",
          "type": "string",
          "placeholder": "192.168.1.50"
        },
        {
          "key": "port",
          "label": "Port",
          "type": "int",
          "min": 1,
          "max": 65535
        },
        {
          "key": "mode",
          "label": "Mode",
          "type": "string",
          "placeholder": "raw_tcp|ipp|lpr"
        }
      ]
    },
    {
      "id": "autoload",
      "title": "Autoload",
      "fields": [
        {
          "key": "enabled",
          "label": "Enabled",
          "type": "bool"
        },
        {
          "key": "autoload",
          "label": "Autoload on boot",
